

options:
	timewait: 6 seconds

command /emotes [<text>]:
	trigger:	
		if arg-1 is not set:
			message "&7&m-------------------------------"
			message "      &e&lEmotes by Snap"
			message ""
			message "      &b/emotes &7(&b1&7-&b3&7)"
			message "&7&m-------------------------------"
			stop
		if arg-1 is "1":
			message "&7&m-------------------------------"
			message " &8- &b/hug <player> &7Share a hug"
			message " &8- &b/judge <player> &7Rate a player"
			message " &8- &b/mad <player> &7You mad bro?"
			message " &8- &b/love <player> &7Share the love"
			message " &8- &b/angry &7Feeling salty?"
			message " &8- &b/happy &7Clap your hands"
			message " &8- &b/fear &7Are you afraid?"
			message " &8- &b/sad &7Not so happy?"
			message " &8- &b/r00d &7That's r00d"
			message "&7&m-------------------------------"
			stop
			
		else if arg-1 is "2":
			message "&7&m-------------------------------"
			message " &8- &b/annoyed <player> &7Are you annoyed?"
			message " &8- &b/confused &7What?"
			message " &8- &b/tired &7Sleepy?"
			message " &8- &b/triggered &7Oh wow"
			message " &8- &b/crush <player> &7That special somone"
			message " &8- &b/lost &7Umm sorry come again"
			message " &8- &b/moist &7Sticky?"
			message "&7&m-------------------------------"
			stop
		else if arg-1 is "3":
			message "&7&m-------------------------------"
			message " &8- &b/eat &7Hungry?"
			message " &8- &b/sit &7Need a break?"
			message " &8- &b/suicide &7Are we done here?"
			message " &8- &b/gyar &7Gyar the bear?"
			message " &8- &b/nub &7What a nub"
			message " &8- &b/fact &7This is a command"
			message " &8- &b/cry &7Don't cry you big baby"
			message "&7&m-------------------------------"
			
		else:
			message "&7&m-------------------------------"
			message "	  &e&lEmotes by Snap"
			message ""
			message "	   &b/emotes &7(&b1&7-&b3&7)"
			message "&7&m-------------------------------"
		
			
on load:
	delete {justusedemote::*} 
command /emotetoggle:
	trigger:
		if {emote.toggle::%uuid of player%} is not set:
			set {emote.toggle::%uuid of player%} to false
			message "&8[&bEmotes&8] &cPlayer's can no longer use emote commands on you."
		else:
			delete {emote.toggle::%uuid of player%}
			message "&8[&bEmotes&8] &aPlayer's can now use your name in emotes again."
 
command /hug [<text>]:
	trigger:
		if player does not have permission "emote.member":
			message "&8[&bEmotes&8] &7You need a higher rank to use that"
			stop
		if {justusedemote::%player%} is not set:
			set {justusedemote::%player%} to now
		else if {justusedemote::%player%} was less than 6 seconds ago:
			message "&8[&bEmote&8] &7Please wait a few seconds before doing another emote"
			stop
		if arg-1 is not set:
			message "&8[&bEmotes&8] &7Please add a player &b/hug (player)"
			stop
		set {justusedemote::%player%} to now
		if "%arg-1%" = "all" or "everyone":
			chance of 25%:
				broadcast "&b» &e%player's name% &ajust gave you a hug"
				stop
			chance of 25%:
				broadcast "&b» &e%player's name% &ajust gave everyone a hug"
				stop
			chance of 25%:
				broadcast "&b» &e%player's name% &ajust gave a group hug... to everyone!"
				stop
			chance of 25%:
				broadcast "&b» &e%player's name% &ahugged everyone"
				stop
			broadcast "&b» &e%player's name% &agave you a big hug <3"
			stop
		loop all players:
			if "%arg-1%" = "%loop-player%":
				set {_target} to loop-player
		if {_target} is not set:
			message "&8[&bEmotes&8] &7Try /hug (name)"
			stop
		if "%{_target}%" is "<none>":
			message "&8[&bEmotes&8] &7Player not found. Try /hug (name)"
			if player has permission "is.op":
				message "%{_target}%"
			stop
		if {emote.toggle::%{uuid::%{_target}%}%} is set:
			message "&8[&bEmotes&8] &eThat player has emotes toggled off"
			stop
		if "%player%" = "%{_target}%":
			chance of 25%:
				broadcast "&b» &e%player's name% &ajust tried to hug themself..."
				stop
			chance of 25%:
				broadcast "&b» &e%player's name% &ajust gave themself a hug"
				stop
			chance of 25%:
				broadcast "&b» &e%player's name% &ajust hugged themself..."
				stop
			broadcast "&b» &e%player's name% &ahugged themself &d:("
			stop
		chance of 25%:
			broadcast "&b» &e%player's name% &agave a hug to &e%{_target}%"
			stop
		chance of 25%:
			broadcast "&b» &e%player's name% &agave a big hug to &e%{_target}%"
			stop
		chance of 25%:
			broadcast "&b» &e%player's name% &agave a giant hug to &e%{_target}%"
			stop
		chance of 25%:
			broadcast "&b» &e%player's name% &agave a bear hug to &e%{_target}%"
			stop
		chance of 25%:
			broadcast "&b» &e%player's name% &agave a cute hug to &e%{_target}%"
			stop
		chance of 25%:
			broadcast "&b» &e%player's name% &agave a lovely hug to &e%{_target}%"
			stop
		chance of 25%:
			broadcast "&b» &e%player's name% &agave a crushing hug to &e%{_target}%"
			stop
		broadcast "&b» &e%player's name% &agave a hug to &e%{_target}%"
		
		
	
 
